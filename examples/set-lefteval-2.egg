do { 
  def (x, array(array(1,2),array(3,4))),
  set(x[0], 9), # [9, [3,4]] # Ideally set(x[0], 9)
  print(x), # [ 9, [ 3, 4 ] ]
  
  def(y, map{x:4, y: array(0,7)}),
  set(y[y:][1], 9), # Ideally set(y["y"][1], 9) map{x:4, y: [0,9]}
  print(y["y"][1]), # 9
  print(y), # { x: 4, y: [ 0, 9 ] }

  def(z, object{
           c:4, 
           g: fun(a, set(self.c, a)) # Ideally set(self.c,a)
          }
  ),
  set(z.c, 12),
  print(z.c),    # 12
  print(z.g(8)), # 8
  print(z.c)     # 8
}
